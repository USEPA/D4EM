<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0,Profile=Client"/>
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="StationServicePortBinding" />
                <binding name="StoretResultServicePortBinding" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://ofmpub.epa.gov:80/STORETwebservices/StationService/StationService"
                binding="basicHttpBinding" bindingConfiguration="StationServicePortBinding"
                contract="StationServiceReference.StationService" name="StationServicePort" />
            <endpoint address="http://ofmpub.epa.gov:80/STORETwebservices/StoretResultService/StoretResultService"
                binding="basicHttpBinding" bindingConfiguration="StoretResultServicePortBinding"
                contract="ResultServiceReference.StoretResultService" name="StoretResultServicePort" />
        </client>
    </system.serviceModel>
</configuration>
