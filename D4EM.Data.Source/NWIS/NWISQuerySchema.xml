  <function name='GetNWISStations' label='NWIS Stations'>
    <arguments>
      <DataType       format='String'   label='Type of stations to get' valid='discharge|qw|gw|measurements|peak' optional='true' default='discharge' />
      <region         format='Region'   label='Region to download' />
      <ParameterCode  format='string*5' label='NWIS Parameter Code'   optional='true' default='' />
      <MinCount       format='Integer'  label='Minimum number of data values' optional='true' default='0' />
      <SaveAs         format='saveas'   label='Save list of stations as' filter='Text File (*.txt)|*.txt' default='DefaultStationsFilename'/>
    </arguments>
    <returns>
      <usgs_stations format='text' label='USGS Stations' />
    </returns>
  </function>
  <function name='GetNWISStatistics' label='NWIS Statistics'>
    <arguments>
      <HUC    format='string' label='2-digit or 8-digit HUC' optional='false' />
      <SaveIn format='folder' label='Save data in folder' />
    </arguments>
    <returns>
      <NWISDailyDischarge format='text' label='NWIS Daily Discharge Values' />
    </returns>
  </function>
