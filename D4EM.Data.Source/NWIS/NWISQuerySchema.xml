<DataExtension name='NWISDataExtension'>
  <function name='GetNWISDailyDischarge' label='NWIS Discharge'>
    <arguments>
      <StartDate format='datestring' label='Starting date for values to retrieve' optional='true' default='1/1/1900' />
      <EndDate   format='datestring' label='Ending date for values to retrieve'   optional='true' default='1/1/2100' />
      <StationID format='string' label='Station ID'                               optional='false' multiple='true' />
      <SaveIn    format='folder' label='Save data in folder' filter='Text Files (*.txt)|*.txt' />
    </arguments>
    <returns>
      <NWISDailyDischarge format='text' label='NWIS Daily Discharge Values' />
    </returns>
  </function>
